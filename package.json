{
  "name": "customerdetails",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@sap/cds": "^9",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "cds-plugin-ui5": "^0.13.0"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": "my.db"
        }
      },
      "auth": {
      "kind": "mocked",
      "users": {
        "admin@gmail.com": {
          "password": "12345678",
          "roles": [ "customerAdmin"]
        },
        "display@gmail.com": {
          "password": "12345678",
          "roles": [ "customerDisplay"]
        }
      }
    }
    }
  },
  "scripts": {
    "start": "cds-serve",
    "watch-customerdetailsui": "cds watch --open customersui.customerdetailsui/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/customerdetailsui"
  ]
}